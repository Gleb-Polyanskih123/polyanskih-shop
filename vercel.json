{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/api/index.js"
    },
    {
      "src": "/register",
      "dest": "/api/index.js"
    },
    {
      "src": "/products",
      "dest": "/api/index.js"
    },
    {
      "src": "/buy",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}